{% extends 'base.html.twig' %}

{% block title %}Fokotany{% endblock %}

{% block body %}
    <h1>Fokotany</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ fokotany.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ fokotany.nom }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ fokotany.code }}</td>
            </tr>
            <tr>
                <th>Commune</th>
                <td>{{ fokotany.commune }}</td>
            </tr>
            <tr>
                <th>District</th>
                <td>
                    {{ fokotany.commune.district }}
                </td>
            </tr>
            <tr>
                <th>District</th>
                <td>
                    {{ fokotany.commune.district.region }}
                </td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_fokotany_index') }}" class="btn btn-outline-primary">back to list</a>

    <a href="{{ path('app_fokotany_edit', {'id': fokotany.id}) }}" class="btn btn-outline-primary">edit</a>

    {{ include('fokotany/_delete_form.html.twig') }}
{% endblock %}
